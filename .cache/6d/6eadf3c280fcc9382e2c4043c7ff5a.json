umn":24}},{"generated":{"line":101,"column":8},"source":"components/Block.js","original":{"line":46,"column":24}},{"generated":{"line":101,"column":15},"source":"components/Block.js","original":{"line":46,"column":31}},{"generated":{"line":101,"column":17},"source":"components/Block.js","original":{"line":46,"column":32}},{"generated":{"line":101,"column":25},"source":"components/Block.js","original":{"line":46,"column":16}},{"generated":{"line":102,"column":0},"source":"components/Block.js","original":{"line":46,"column":41}},{"generated":{"line":102,"column":8},"source":"components/Block.js","original":{"line":46,"column":41}},{"generated":{"line":102,"column":14},"source":"components/Block.js","original":{"line":46,"column":47}},{"generated":{"line":102,"column":16},"source":"components/Block.js","original":{"line":46,"column":48}},{"generated":{"line":102,"column":23},"source":"components/Block.js","original":{"line":46,"column":16}},{"generated":{"line":103,"column":0},"source":"components/Block.js","original":{"line":46,"column":56}},{"generated":{"line":103,"column":8},"source":"components/Block.js","original":{"line":46,"column":56}},{"generated":{"line":103,"column":15},"source":"components/Block.js","original":{"line":46,"column":63}},{"generated":{"line":103,"column":17},"source":"components/Block.js","original":{"line":46,"column":65}},{"name":"toggleTransaction","generated":{"line":103,"column":22},"source":"components/Block.js","original":{"line":46,"column":70}},{"generated":{"line":104,"column":0},"source":"components/Block.js","original":{"line":46,"column":16}},{"generated":{"line":104,"column":22},"source":"components/Block.js","original":{"line":44,"column":12}},{"generated":{"line":104,"column":23},"source":"components/Block.js","original":{"line":43,"column":8}},{"generated":{"line":105,"column":0},"source":"components/Block.js","original":{"line":50,"column":5}},{"name":"Component","generated":{"line":109,"column":2},"source":"components/Block.js","original":{"line":5,"column":20}},{"name":"Block","generated":{"line":111,"column":15},"source":"components/Block.js","original":{"line":68,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\nimport { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\n\nclass Block extends Component {\n    state = { \n        displayTransaction: false\n    }\n\n    toggleTransaction = () => {\n        this.setState({\n            displayTransaction: !this.state.displayTransaction\n        })\n    }\n\n    get displayTransaction() {\n        const { data } = this.props.block;\n\n        const stringifiedData = JSON.stringify(data);\n\n        const dataDisplay = stringifiedData.length > 35 ?\n            `${stringifiedData.substring(0, 35)}...` :\n            stringifiedData;\n        // if display transaction is true, then use full data\n        if (this.state.displayTransaction) {\n            return (\n                <div>\n                    {\n                        data.map(transaction => (\n                            <div key={transaction.id}>\n                                <hr />\n                                <Transaction transaction={transaction} />\n                            </div>\n                        ))\n                    }\n                    <br />\n                    <Button bsStyle='danger' bsSize='small' onClick={this.toggleTransaction}> Show Less\n                    </Button>\n                </div>\n            )\n        } \n        // else if display transaction is false, then use summary of data\n        return (\n            <div> \n                <div>Data: {dataDisplay}</div>\n                <Button bsStyle='danger' bsSize='small' onClick={this.toggleTransaction}> Show More\n                </Button>\n            </div>\n        )\n    }\n    \n    render() {\n        // receive block passed from blocks as a PROP\n        const {timestamp, hash } = this.props.block;\n        // render just part of the hash\n        const hashDisplay = `${hash.substring(0, 15)}...`\n\n        return (\n            <div className='Block'>\n                <div>Hash: {hashDisplay}</div>\n                <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\n                {this.displayTransaction}\n            </div>\n        )\n    }\n}\n\nexport default Block;"},"lineCount":null}},"hash":"8ccdc42dc252d260106f44ef2cca8180","cacheData":{"env":{}}}